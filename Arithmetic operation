package java_pratice;

public class Operation {
	public static void main(String[] agrs) {
		int a=15;
		int b=4;
		int c=a+b;
		System.out.println(c);
		int d=a-b;
		System.out.println(d);
		int e=a*b;
		System.out.println(e);
		int f=a/b;
		System.out.println(f);
		int g=a%b;
		System.out.println(g);
	}
}
